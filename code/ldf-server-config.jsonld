{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "npmd": "http://linkedsoftwaredependencies.org/bundles/npm/",
    "ldfs": "npmd:ldf-server/",
    "rdfa": "http://www.w3.org/ns/rdfa#"
  },
  "@id": "urn:ldfserver:my",
  "@type": "ldfs:Server/Qpf",

  "rdfs:label": "My Linked Data Fragments server",

  "ldfs:datasource": [
    {
      "@id": "ex:myHdtDatasource",
      "@type": "ldfs:Datasource/Hdt",
      "rdfs:label": "DBpedia 2014",
      "rdfs:comment": "DBpedia 2014 with an HDT back-end",
      "ldfs:Datasource/path": "dbpedia",
      "ldfs:Datasource/Hdt/file": "data/dbpedia2014.hdt"
    },
    {
      "@id": "ex:mySparqlDatasource",
      "@type": "ldfs:Datasource/Sparql",
      "rdfs:label": "DBpedia 3.9 (Virtuoso)",
      "rdfs:comment": "DBpedia 3.9 with a Virtuoso back-end",
      "ldfs:Datasource/path": "dbpedia-sparql",
      "ldfs:Datasource/Sparql/endpoint": "http://dbpedia.restdesc.org/",
      "ldfs:Datasource/Sparql/defaultGraph": "http://dbpedia.org"
    }
  ]
}